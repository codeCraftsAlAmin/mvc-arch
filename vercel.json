{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/users",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/index.html", 
      "dest": "/index.html" 
    }
  ]
}
